	<form-beans>
		<form-bean name="student" type="com.bizruntime.bean.StudentFormBean" />
	</form-beans>
	<!-- Mapping -->
	<action-mappings>
		<action path="/regForm" name="student" validate="true" input="/index.jsp" />
